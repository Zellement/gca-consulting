{
    "private": true,
    "overrides": {
        "vue": "latest"
    },
    "scripts": {
        "build": "nuxt build",
        "clean:dsstore": "find . -name '.DS_Store' -depth -delete",
        "dev": "npm run clean:dsstore && nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "npm-run-all --parallel lint:*",
        "lint:js": "npx eslint --no-error-on-unmatched-pattern --fix {components,layouts,pages,plugins,stores,utils,test,middleware}/**/*.{js,ts,vue}",
        "lint:css": "stylelint --config ./.stylelintrc --fix \"assets/**/*.{css,scss}\"",
        "format": "npm-run-all --serial format:check format:write",
        "format:check": "npx prettier --check \"**/*.{js,vue,ts,scss,md,json,html}\"",
        "format:write": "npx prettier --write \"**/*.{js,vue,ts,scss,md,json,html}\"",
        "pull-sb-components": "storyblok pull-components --path storyblok/ --space 311400",
        "generate-sb-types": "storyblok generate-typescript-typedefs --sourceFilePaths storyblok/components.311400.json --destinationFilePath types/storyblok-component-types.d.ts"
    },
    "devDependencies": {
        "@nuxt/fonts": "^0.8.0",
        "@nuxtjs/seo": "^2.0.0-rc.21",
        "@nuxtjs/svg-sprite": "^1.0.2",
        "@nuxtjs/tailwindcss": "^6.12.1",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.9",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-nuxt": "^4.0.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-vue": "^9.28.0",
        "npm-run-all": "^4.1.5",
        "nuxt": "^3.13.1",
        "nuxt-aos": "^1.2.5",
        "nuxt-viewport": "^2.1.6",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "sass": "^1.78.0",
        "stylelint": "^16.9.0",
        "stylelint-config-standard-scss": "^13.1.0",
        "typescript": "^5.6.2",
        "vite-plugin-eslint": "^1.8.1",
        "vue-tsc": "2.0.29"
    },
    "dependencies": {
        "@nuxt/image": "^1.8.0",
        "@pinia/nuxt": "^0.5.4",
        "esbuild": "^0.23.1",
        "pinia": "^2.2.2",
        "stylelint-prettier": "^5.0.2",
        "vue-dompurify-html": "^5.1.0",
        "@storyblok/js": "^3.0.8"
    }
}
